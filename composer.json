{
    "name": "enolaphp/enolaphp",
    "description": "Aplicacion con Framework Enola PHP.",
    "keywords": ["framework", "enolaphp"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/edunola13/enolaphp/issues",
        "source": "https://github.com/edunola13/enolaphp"
    },
    "authors": [
        {
            "name": "Eduardo Sebastian Nola",
            "email": "edunola13@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "enolaphp/framework": "dev-master",
        "enolaphp/utils": "dev-master"
    },
    "require-dev": {        
    },
    "autoload": {
        "psr-4": {
            "YourApp\\Libs\\": "app/libs/",
            "YourApp\\Controllers\\": "app/src/controllers",
            "YourApp\\Filters\\": "app/src/filters"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev"
}
