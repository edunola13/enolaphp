{
    "defaultStore": "File",
    "prefix": "enolaphp",
    "stores":{
        "File":{
            "driver": "file",
            "folder": "cache"
        },
        "DataBase":{
            "driver": "database",
            "connection": "Local",
            "table": "cache"
        },
        "Apc":{
            "driver": "apc"
        },
        "Memcached":{
            "driver": "memcached",
            "servers":{
                "server1":{"host": "localhost", "port": "11211", "weight": "0"}
            }
        },
        "Redis":{
            "driver": "redis",
            "schema": "tcp",
            "host": "localhost",
            "port": "6379"
        }
    }
}